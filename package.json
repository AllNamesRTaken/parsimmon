{
  "name": "parsimmon",
  "version": "1.18.1",
  "description": "A monadic LL(infinity) parser combinator library",
  "keywords": [
    "parsing",
    "parse",
    "parsers",
    "parser combinators"
  ],
  "author": "Joel Brage",
  "contributors": [
    "Jeanine Adkisson <jneen at jneen dot net>",
    "Brian Mock <brian@mockbrian.com>"
  ],
  "repository": "https://github.com/allnamesrtaken/parsimmon",
  "files": [
    "src",
    "build"
  ],
  "type": "module",
  "main": "src/parsimmon.js",
  "browser": "build/parsimmon.umd.min.js",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-chai-expect": "^3.1.0",
    "eslint-plugin-mocha": "^11.1.0",
    "mkdirp": "^3.0.1",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "git push --follow-tags && npm publish",
    "prebuild": "rimraf build && mkdirp build",
    "prepublishOnly": "npm run test && npm run build",
    "lint": "prettier --list-different '{examples,src,test}/**/*.js' && eslint examples src test",
    "lint:fix": "prettier --write '{examples,src,test}/**/*.js' && eslint --fix examples src test",
    "coverage": "rimraf coverage && vitest run --coverage",
    "pretest": "npm run lint",
    "test:dev": "vitest",
    "test": "vitest run"
  }
}
